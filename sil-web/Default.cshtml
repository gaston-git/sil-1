@using System.Collections.Generic;
@using Newtonsoft.Json;
@{
    Page.Title = "SISTEMA DE INFORME DE LABORES (SIL)";
    Layout = "~/_Layout.cshtml";

    wsServicios.Service1 obj = new wsServicios.Service1();
    var datos = JsonConvert.DeserializeObject<List<dynamic>>(obj.getRegistros("0"));
   
}
<br />
<div class="contenedor">
    <button type="button" id="btnNuevo" class="btn btn-primary">Nuevo</button>
    <button type="button" id="btnEditar" class="btn btn-default" disabled>Editar</button>
    <button type="button" id="btnEliminar" class="btn btn-default" disabled>Eliminar</button><br /><br />
    <div class="table-responsive">
        <table id="registros" class="table" cellspacing="0" width="100">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Fecha</th>
                    <th>Hora_Inicio</th>
                    <th>Hora_Final</th>
                    <th>Sistema</th>
                    <th>Accion</th>
                    <th>Detalle</th>
                    <th>Observacion</th>
                    <th>%_Avance</th>
                    <th>Estado</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var row in datos)
                {
                    <tr>
                        <td>@row.id</td>
                        <td>@row.Fecha</td>
                        <td>@row.horaInicio</td>
                        <td>@row.horaFinal</td>
                        <td>@row.Sistema</td>
                        <td>@row.Accion</td>
                        <td>@row.detalle</td>
                        <td>@row.observacion</td>
                        <td>@row.avance %</td>
                        <td>@row.Estado</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<script src="~/js/FuncionesGn.js"></script>
